  "dependencies": {
    "@types/body-parser": "^1.16.8",
    "@types/express": "^4.0.39",
    "@types/react": "^16.0.28",
    "@types/react-dom": "^16.0.3",
    "@types/socket.io": "^1.4.31",
    "@types/sqlite3": "^3.1.1",
    "@types/webpack": "^3.8.1",
    "awesome-typescript-loader": "^3.4.1",
    "body-parser": "^1.18.2",
    "chokidar-cli": "^1.2.0",
    "cpx": "^1.5.0",
    "express": "^4.16.2",
    "local-web-server": "^2.3.0",
    "npm-run-all": "^4.1.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "semantic-ui-react": "^0.77.1",
    "socket.io": "^2.0.4",
    "sqlite3": "^3.1.13",
    "trash-cli": "^1.4.0",
    "ts-node": "^3.3.0",
    "typescript": "^2.6.2",
    "webpack": "^3.9.1"
  },
  "scripts": {
    "preinit": "trash ./db.db",
    "init": "ts-node ./src/server/Init.ts",
    "prestart": "run-s build",
    "start": "run-p serve watch",
    "serve": "ts-node ./src/server/App.ts",
    "build": "run-p build:*",
    "build:app": "run-p build:app:*",
    "build:app:html": "cpx \"./src/webapp/*.html\" \"./pub/\"",
    "build:app:ts": "webpack",
    "watch": "run-p watch:*",
    "watch:app": "run-p watch:app:*",
    "watch:app:ts": "chokidar \"./src/webapp\" -c \"yarn run build:app:ts\""
  }
}
